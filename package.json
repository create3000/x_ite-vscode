{
  "name": "x_ite-vscode",
  "displayName": "X_ITE VS Code Extension",
  "description": "Preview and debug X3D, glTF, VRML and other 3D models directly in the editor.",
  "version": "0.0.1",
  "repository": {
      "url": "https://github.com/create3000/x_ite-vscode"
  },
  "keywords": [
    "X3D",
    "3D",
    "model"
  ],
  "galleryBanner": {
    "color": "#383838",
    "theme": "dark"
  },
  "engines": {
    "vscode": "^1.93.0"
  },
  "categories": [
    "Debuggers"
  ],
  "activationEvents": [],
  "main": "./extension.js",
  "contributes": {
    "commands": [{
      "command": "x_ite-vscode.preview-model",
      "title": "X3D: Preview 3D Model"
    }]
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/vscode": "^1.93.0",
    "@types/mocha": "^10.0.8",
    "@types/node": "20.x",
    "eslint": "^9.11.1",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.4.1"
  }
}
